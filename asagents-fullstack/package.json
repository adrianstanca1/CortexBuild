{
  "name": "asagents-fullstack",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "backend:dev": "cd backend && npm run dev",
    "backend:build": "cd backend && npm run build",
    "java:dev": "cd java-backend && ./mvnw spring-boot:run",
    "java:build": "cd java-backend && ./mvnw clean package",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\" \"npm run java:dev\"",
    "setup": "npm install && cd backend && npm install && cd ../java-backend && ./mvnw clean install"
  },
  "dependencies": {
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-query": "^3.39.3",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.2.0",
    "@heroicons/react": "^2.0.18",
    "recharts": "^2.8.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "@types/leaflet": "^1.9.8"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "typescript": "^5.3.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "concurrently": "^8.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10"
  }
}
