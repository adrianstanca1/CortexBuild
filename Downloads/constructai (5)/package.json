{
  "name": "constructai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && node deploy-ionos.js",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel",
    "vercel:prod": "vercel --prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules node_modules/.vite",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "prepare": "husky",
    "db:connect": "tsx scripts/db-connect.ts",
    "db:stats": "tsx scripts/db-stats.ts",
    "db:backup": "cp cortexbuild.db \"cortexbuild-backup-$(date +%Y%m%d-%H%M%S).db\" && echo 'âœ… Backup created'",
    "db:restore": "echo 'Usage: cp cortexbuild-backup-YYYYMMDD-HHMMSS.db cortexbuild.db'"
  },
  "dependencies": {
    "@google/genai": "^1.22.0",
    "@hookform/resolvers": "5.2.2",
    "@supabase/supabase-js": "2.44.4",
    "@types/bcryptjs": "2.4.6",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jsonwebtoken": "9.0.10",
    "@types/uuid": "10.0.0",
    "@vercel/postgres": "0.10.0",
    "axios": "1.12.2",
    "bcryptjs": "3.0.2",
    "better-sqlite3": "12.4.1",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "express": "5.1.0",
    "framer-motion": "12.23.24",
    "ftp": "0.3.10",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.544.0",
    "openai": "6.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "7.65.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "7.9.4",
    "recharts": "3.3.0",
    "sqlite3": "5.1.7",
    "uuid": "13.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@tailwindcss/postcss": "4.1.14",
    "@types/node": "^22.14.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.46.0",
    "@typescript-eslint/parser": "8.46.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "cssnano": "7.1.1",
    "eslint": "9.37.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "6.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.14",
    "tsx": "4.20.6",
    "typescript": "~5.8.2",
    "vercel": "48.2.6",
    "vite": "^6.2.0"
  }
}
