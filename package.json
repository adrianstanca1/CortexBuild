    "better-sqlite3": "12.4.1",
    "cors": "2.8.5",
    "dotenv": "17.2.3",
    "esbuild": "0.25.11",
    "express": "5.1.0",
    "ftp": "0.3.10",
    "jsonwebtoken": "9.0.2",
