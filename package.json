{
  "name": "constructai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "dev:vite": "vite",
    "server": "tsx --env-file=.env server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "next build",
    "preview": "next build && next start -p 3000",
    "deploy": "npm run build && node deploy-ionos.cjs",
    "vercel:dev": "vercel dev",
    "vercel:deploy": "vercel",
    "vercel:prod": "vercel --prod",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "e2e": "playwright test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "scan:secrets": "bash scripts/scan-secrets.sh",
    "db:verify:supabase": "tsx scripts/verify-supabase.ts",
    "db:init:sqlite": "tsx scripts/sqlite-init.ts"
  },
